    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.fwchen.jellyfish",
      "supportsTablet": false
    },
    "android": {
      "package": "org.fwchen.jellyfish"
    }
  }
}
