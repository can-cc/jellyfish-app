{
  "expo": {
    "name": "水母清单",
    "slug": "jellyfish-todo",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.18",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "backgroundColor": "#fff",
      "resizeMode": "contain"
